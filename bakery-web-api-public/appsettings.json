{
  "ConnectionStrings": {
    "bakeryDbCon": "${BAKERYDBCON}",
    "blobContainerCon": "${BLOBCONTAINERCON}",
    "pageUrl": "${PAGEURL}"
  },
  "OAuth": {
    "clientId": "${CLIENTID}",
    "clientSecret": "${CLIENTSECRET}",
    "redirectUri": "${REDIRECTURIGMAIL}"
  },
  "MetaDev": {
    "instagramKey": "${INSTAGRAMKEY}",
    "ID": "${ID}",
    "AppSecret": "${APPSECRET}",
    "redirectUri": "${REDIRECTURIFACEBOOK}"
  },
  "SmtpSettings": {
    "SmtpServer": "${SMTPSERVER}",
    "Port": 587,
    "Username": "${USERNAME}",
    "Password": "${PASSWORD}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Urls": "http://127.0.0.1:5024",
  "Jwt": {
    "SecretKey": "${JWT_SECRETKEY}"
  },
  "BlobContainerString": {
    "blobKey": "${BLOBKEY}"
  }
}
